{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["sonarjs"],
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"]
    },
    "globals": {
        "process": true
    }
}
